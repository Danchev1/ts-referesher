{
  "name": "ts-playground",
  "version": "1.0.0",
  "license": "ISC",
  "author": "Yordan Danchev",
  "description": "Playground for pure TS, no frameworks",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danchev1/ts-referesher.git"
  },
  "scripts": {
    "start:dev": "webpack-dev-server --config webpack/webpack.config.dev.js",
    "build:prod": "webpack --config webpack/webpack.config.prod.js",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "eslint": "eslint src --ext .ts"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "**/*.{ts,js}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/webpack/**"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
